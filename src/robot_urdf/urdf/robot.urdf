<?xml version="1.0"?>
<robot name="robot">

	<link name="base_footprint">
		<origin rpy="0 0 0" xyz="0 0 0"/>
	</link>

	<link name="base_link">
		<visual>
			<geometry>
				<box size="0.360 0.290 0.070"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<material name="yellow">
				<color rgba="0.8 0.8 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="0.360 0.290 0.070"/>
			</geometry>
		</collision>
	</link>
	
	<joint name="base_footprint_joint" type="fixed">
		<parent link="base_footprint"/>
		<child link="base_link"/>
		<origin rpy="0 0 0" xyz="0 0 0.035"/>
	</joint>

	<link name="l_rear_wheel">
		<visual>
			<geometry>
				<cylinder length="0.065" radius="0.035"/>
			</geometry>
			<origin rpy="1.57075 0 0" xyz="0 0 0"/>
			<material name="black">
				<color rgba="0.05 0.05 0.05 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<cylinder length="0.065" radius="0.035"/>
			</geometry>
		</collision>
	</link>

	<joint name="l_rear_wheel_joint" type="continuous">
		<parent link="base_link"/>
		<child link="l_rear_wheel"/>
		<origin xyz="-.1475 .1775 0" rpy="0 0 0" />
		<axis xyz="0 1 0" rpy="0 0 0" />
	</joint>

	<link name="l_front_wheel">
		<visual>
			<geometry>
