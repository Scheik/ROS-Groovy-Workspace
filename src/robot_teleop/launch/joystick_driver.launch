<launch>
   <!-- Load parameters for joy_node -->
   <rosparam
     command="load"
     ns="/joy_node"
     file="$(find robot_teleop)/config/joystick_defaults.yaml"
   />
   <!-- Run joy_node -->
   <node 
     name="joy_node" 
     pkg="joy"   
     type="joy_node"
     required="true"
     output="screen"
   />
   <!-- Run joystick_teleop node -->
   <node
     name="joystick_teleop"
     pkg="robot_teleop"
     type="joystick_teleop"
     required="true"
     output="screen"
   />
</launch>
